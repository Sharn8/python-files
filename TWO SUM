      "cell_type": "code",
      "source": [
        "def twosum(nums,target):\n",
        "  hashmap ={}\n",
        "  for i, num in enumerate(nums):\n",
        "    complement = target-num\n",
        "\n",
        "    if complement in hashmap:\n",
        "         return [hashmap[complement],i]\n",
        "\n",
        "    hashmap[num]=i\n",
        "  return[]\n",
        "\n",
        "print(twosum([2,7,11,15],9))\n",
        "print(twosum([3,2,4],6))\n",
        "print(twosum([3,3],6))"
      ],
